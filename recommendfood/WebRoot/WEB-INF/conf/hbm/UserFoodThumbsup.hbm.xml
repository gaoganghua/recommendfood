<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class name="com.recommendfood.model.UserFoodThumbsup" table="user_food_thumbsup" >
	<id name="id" column="id" type="java.lang.Long">
		<generator class="native"></generator>
	</id>
	<property name="userId" column="user_id" type="java.lang.Long"></property>
	<property name="foodId" column="food_id" type="java.lang.Long"></property>
	<property name="type" column="type" type="java.lang.Byte"></property>
	<property name="thumbsupTime" column="thumbsup_time" type="java.util.Date"></property>
</class>
</hibernate-mapping>

