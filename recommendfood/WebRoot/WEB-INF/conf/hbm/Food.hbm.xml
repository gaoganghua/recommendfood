<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class name="com.recommendfood.model.Food" table="food" >
	<id name="id" column="id" type="java.lang.Long">
		<generator class="native"></generator>
	</id>
	<property name="name" column="name" type="java.lang.String"></property>
	<property name="pictures" column="pictures" type="java.lang.String"></property>
	<property name="stuff" column="stuff" type="java.lang.String"></property>
	<property name="introduction" column="introduce" type="java.lang.String"></property>
	<property name="seasoning" column="seasoning" type="java.lang.String"></property>
	<property name="manufacturingProcess" column="manufacturing_process" type="java.lang.String"></property>
	<property name="status" column="status" type="java.lang.Integer"></property>
	<property name="foodType" column="food_type" type="java.lang.String"></property>
</class>
</hibernate-mapping>

