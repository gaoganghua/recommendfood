<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class name="com.recommendfood.model.NoteReply" table="note_reply" >
	<id name="id" column="id" type="java.lang.Long">
		<generator class="native"></generator>
	</id>
	<property name="noteId" column="note_id" type="java.lang.Long"></property>
	<property name="userId" column="user_id" type="java.lang.Long"></property>
	<property name="replyMsg" column="reply_msg" type="java.lang.String"></property>
	<property name="replyTime" column="reply_time" type="java.util.Date"></property>
	<property name="read" column="read" type="java.lang.Boolean"></property>
	<property name="parentId" column="parent_id" type="java.lang.Long"></property>
	<property name="floorNumber" column="floor_number" type="java.lang.Integer"></property>
</class>
</hibernate-mapping> 

