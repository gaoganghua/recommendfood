<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class name="com.recommendfood.model.AdminLog" table="admin_log" >
	<id name="id" column="id" type="java.lang.Long">
		<generator class="native"></generator>
	</id>
	<property name="logMsg" column="log_msg" type="java.lang.String"></property>
	<property name="logTime" column="log_time" type="java.util.Date"></property>
</class>
</hibernate-mapping> 

